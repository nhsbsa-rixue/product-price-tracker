{
  "name": "product-price-tracker",
  "version": "1.0.0",
  "description": "A personal assignment to create a RESTful web service to users to track product prices",
  "main": "dist/src/index.js",
  "scripts": {
    "preinstall": "node -e \"process.platform === 'darwin' && require('child_process').spawn('sh', ['./npm_scripts/install_pre_commit.sh'], { stdio: 'inherit'})\" && node -e \"process.platform === 'win32' && require('child_process').spawn('cmd.exe', ['/c','call npm_scripts/install_pre_commit.bat'], { stdio: 'inherit'})\"",
    "postinstall": "npm run build",
    "build": "rimraf ./dist && tsc",
    "esbuild": "rimraf ./dist && npx ts-node build.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "validate": "npm run eslint && npm run audit && npm run test",
    "audit": "npm audit --omit=dev",
    "eslint": "eslint .",
    "auto-fix": "eslint --fix .",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "author": "Riwang Xue",
  "license": "Apache-2.0",
  "dependencies": {
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "node-cron": "^4.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@types/aws-lambda": "^8.10.140",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0"
  }
}