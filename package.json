{
  "name": "product-price-tracker",
  "version": "1.0.0",
  "description": "A personal assignment to create a RESTful web service to users to track product prices",
  "main": "dist/src/server.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "rimraf ./dist && tsc",
    "esbuild": "rimraf ./dist && npx ts-node build.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "validate": "npm run eslint && npm run audit && npm run test",
    "audit": "npm audit --omit=dev",
    "eslint": "eslint .",
    "auto-fix": "eslint --fix .",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "deploy-docker": "docker build -t ui-scaffold .",
    "postdeploy-docker": "docker run -p 8001:8001 ui-scaffold"
  },
  "author": "Riwang Xue",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "i18next": "^25.5.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.0",
    "nhsuk-frontend": "^10.0.0",
    "node-cron": "^4.2.1",
    "nunjucks": "^3.2.4",
    "redis": "^5.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0"
  }
}
